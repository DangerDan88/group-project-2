<div id="challenge">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>

        <div class="row justify-center" id="select-character">
          {{#each all_heroes}}
          {{> heroes/heroes-card}}
          {{/each}}
        </div>

        <img src="/img/vs.png">

        <div class="p-10">
          <label class="block">
            <span class="text-gray-700">Select A Challenger</span>
            <select class="form-multiselect block w-full mt-1">
              <option> {{email}} </option>
            </select>
          </label>
        </div>

      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>